{
    "presets": [
        "@babel/preset-typescript",
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": true
                }
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver", {
                "root": ["."],
                "alias": {
                    "~": "./dist",
                    "underscore": "lodash"
                }
            }
        ]
    ]
}
